<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://www.netbeans.org/ns/project/1">
    <type>org.netbeans.modules.j2ee.ejbjarproject</type>
    <configuration>
        <data xmlns="http://www.netbeans.org/ns/j2ee-ejbjarproject/3">
            <name>factura-services</name>
            <minimum-ant-version>1.6.5</minimum-ant-version>
            <included-library dirs="100">reference.factura-core.dist</included-library>
            <included-library dirs="200">file.reference.MITyCLibAPI-1.0.4.jar-1</included-library>
            <included-library dirs="200">file.reference.MITyCLibCert-1.0.4.jar-1</included-library>
            <included-library dirs="200">file.reference.MITyCLibTSA-1.0.4.jar-1</included-library>
            <included-library dirs="200">file.reference.MITyCLibXADES-1.0.4.jar-1</included-library>
            <included-library dirs="200">file.reference.commons-codec-1.9.jar-1</included-library>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <references xmlns="http://www.netbeans.org/ns/ant-project-references/1">
            <reference>
                <foreign-project>factura-core</foreign-project>
                <artifact-type>jar</artifact-type>
                <script>build.xml</script>
                <target>dist</target>
                <clean-target>clean</clean-target>
                <id>dist</id>
            </reference>
        </references>
    </configuration>
</project>
